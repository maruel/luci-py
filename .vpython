python_version: "2.7"

wheel: <
  name: "infra/python/wheels/pycrypto/${platform}_${py_python}_${py_abi}"
  version: "version:2.6.1"
  match_tag: <
    abi: "cp27mu"
    platform: "manylinux1_x86_64"
  >
  match_tag: <
    platform: "macosx_10_6_intel"
  >
>
